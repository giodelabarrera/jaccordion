d":{"line":74,"column":0},"source":"src/Accordion.js","original":{"line":44,"column":9}},{"generated":{"line":74,"column":3},"source":"src/Accordion.js","original":{"line":44,"column":2}},{"generated":{"line":75,"column":0},"source":"src/Accordion.js","original":{"line":47,"column":1}}],"sources":{"src/Accordion.js":"\nclass Item {\n  constructor(title, content, onClick) {\n    this.title = title;\n    this.content = content;\n    this.onClick = onClick;\n\n    title.addEventListener('click', function() {\n      this.onClick(this.title, this.content);\n    }.bind(this));\n  }\n}\n\nclass Accordion {\n  constructor(element) {\n    const children = Array.from(element.children);\n    const titles = children.filter(child => child.nodeName === 'DT');\n    \n    this.items = titles.map(title => {\n      const content = title.nextElementSibling;\n      // const item = new Item(title, content, this.onToggleClick)\n      const item = createItem(title, content, this.onToggleClick)\n      return item;\n    })\n  }\n\n  onToggleClick(title, content) {\n    content.classList.toggle('is-expanded');\n  }\n}\n\nfunction createItem(title, content, onClick) {\n  const item = new Item(title, content, onClick);\n\n  return {\n    getTitle: () => item.title,\n    getContent: () => item.content,\n  }\n}\n\nexport default function createAccordion(element) {\n  const accordion = new Accordion(element);\ndebugger;\n  return {\n    getItems: () => accordion.items\n  }\n}"},"lineCount":null}},"hash":"b6bf024e358555e2f7de0d32cfa93cfb","cacheData":{"env":{}}}